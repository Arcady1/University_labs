rnd EQU 100				// rnd - метка, EQU, 100 - http://codingrus.ru/readarticle.php?article_id=898
					// rnd = 100, а далее += 1 и 2, чтобы задавать сид
	GENERATE (uniform(rnd,0,24))
	GATE U 1,metka1			// http://codingrus.ru/readarticle.php?article_id=869
	GATE U 2,metka2			// если касса 2 свободна, то идем туда
	TEST LE Q1,Q2,metka2		// http://codingrus.ru/readarticle.php?article_id=892
					// Q - текущая длина очереди НОМЕР 1, НОМЕР 2
					// если первая 1-ая оч. <= 2-ой, то движемся по коду дальше, иначе - переходим в metka2
metka1 QUEUE 1				// встали в очередь НОМЕР 1
	SEIZE 1				// моделирование занятия кассы посетителем КОДОМ 1
	DEPART 1			// обслуживание кассы НОМЕР 1
	ADVANCE (uniform(rnd+1,9,24))	// время обслуживания на кассе (задержка)
	RELEASE 1			// моделирование освобождения кассы с КОДОМ 1 посетителем
	TERMINATE			// выход из модели, если обслужили
metka2 QUEUE 2
	SEIZE 2
	DEPART 2
	ADVANCE (uniform(rnd+2,5,24))
	RELEASE 2
	TERMINATE

	GENERATE 3600			// продолжительность выполнения 3600 с
	TERMINATE 1			// кажд. раз уменьш. значение глобального счётчикак на 1
	START 1				// стартовое кол-во секунд


// ADVANCE - отвечает за временные характеристики процесса, в данном случае: 
// uniform(stream, min, max): stream – номер генератора случайных чисел, min -  минимальное генерируемое значение. Автоматически целое. max - максимальное генерируемое значение. Автоматически целое.
// TERMINATE - http://codingrus.ru/readarticle.php?article_id=891

// https://habr.com/ru/post/192044/
